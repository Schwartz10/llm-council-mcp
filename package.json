{
  "name": "second-brain",
  "version": "1.0.0",
  "description": "Multi-model AI deliberation CLI tool",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "second-brain": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:providers": "tsx src/index.ts --test-providers",
    "test:provider": "tsx src/index.ts --test-provider",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schwartz10/second-brain.git"
  },
  "keywords": [
    "ai",
    "llm",
    "cli",
    "multi-model"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schwartz10/second-brain/issues"
  },
  "homepage": "https://github.com/Schwartz10/second-brain#readme",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.18",
    "@ai-sdk/groq": "^3.0.12",
    "@ai-sdk/openai": "^3.0.14",
    "@ai-sdk/xai": "^3.0.29",
    "ai": "^6.0.44",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "ora": "^9.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.17"
  }
}
